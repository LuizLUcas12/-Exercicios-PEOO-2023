soma1 = int(input("A soma de: "))
soma2 = int(input("Mais: "))
soma = soma1 + soma2
print("A soma e igual:", soma)


subt1 = int(input("A subtracao de: "))
subt2 = int(input("Menos: "))
subt = subt1 - subt2
print("A subtracao e igual:", subt)

mult1 = int(input("A multiplicao de: "))
mullt2 = int(input("Vezes: "))
mult = mult1 * mullt2
print("A multiplicacao e igual:",mult)
